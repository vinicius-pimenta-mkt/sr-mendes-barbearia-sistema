{
  "version": 2,
  "builds": [
    {
      "src": "DashboardBarber/src/main.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "DashboardBarber/src/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "DashboardBarber/src/main.py"
    }
  ],
  "installCommand": "pip install -r DashboardBarber/requirements.txt"
}


