{
  "version": 2,
  "builds": [
    {
      "src": "DashboardBarber/src/main.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "DashboardBarber/src/main.py"
    }
  ],
  "buildCommand": "pip install -r DashboardBarber/requirements.txt"
}


